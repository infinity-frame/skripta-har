<header <%if (imageUrl) {%>
  style="background-image: url(<%=imageUrl%>)"<%}%>>
  <div class="content">
    <h1><%=heading%></h1>
    <%if (subtitle) {%>
    <p><%=subtitle%></p>
    <%}%> <%if (button) {%>
    <button onclick="window.location.href = '/'">
      <span class="material-symbols-rounded">arrow_back</span>
      Zpět na hlavní stránku
    </button>
    <%}%>
  </div>
</header>
